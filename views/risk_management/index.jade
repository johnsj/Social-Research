extends ../layout

block scripts
  != js("jquery-1.8.0.min")
  != js("underscore-min")
  != js("backbone-min")
  != js("application.js")

block content
  h1= title
  p Welcome to #{title}
  #container
    #category-form-container

block template-scripts
  script(type="text/template", id="category-template")
    div#category_single
      span <%=title%>
      span <%=description%>
  include _form